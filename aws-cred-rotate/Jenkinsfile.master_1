pipeline {
  agent {
    node {
      label "master"
    }
  }
  stages {
    stage("Rotate keys") {
      steps {
         withAWS( credentials: 'main-jenkins', region: 'us-west-2') {
           sh 'aws-cred-rotate/rotate_master_1.sh -i test1-jenkins -u test1-jenkins -d 0'
         }
      }
    }
  }
}
